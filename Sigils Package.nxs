{"name":"sigils","enabled":true,"description":"","type":"group","id":1,"items":[{"type":"function","name":"SystemLoaded","enabled":true,"id":2,"code":"client.sigils = {};\n\nclient.sigils.dict = {\n    mono: 'monolith',\n    mush: 'mushroom',\n};\n\nclient.sigils.throw = function(sigil, direction, container) {\n    sigil = client.sigils.dict[sigil] || sigil;\n\n    client.EqBalQueue\n            .add('stand')\n            .add('get ' + sigil+ ' from ' +container)\n            .add('wield '+sigil)\n            .add('throw ' +sigil+ ' ' +direction)\n            .add('combine ' +sigil)\n            .add('put group ' +sigil+ ' in ' +container)\n            .send();\n};\n\nclient.sigils.store = function(sigil, container) {\n    sigil = client.sigils.dict[sigil] || sigil;\n\n    client.EqBalQueue\n            .add('stand')\n            .add('get group ' + sigil+ ' from ' +container)\n            .add('get group ' + sigil+ ' from ' +container)\n            .add('get ' + sigil+ ' from ' +container)\n            .add('combine ' +sigil)\n            .add('put group ' +sigil+ ' in ' +container)\n            .send();\n};\n\nclient.sigils.drop = function(sigil, container) {\n    sigil = client.sigils.dict[sigil] || sigil;\n    \n    if(client.sys.isSlowMode()) {\n        client.sendCmd('drop ' +sigil);\n    }\n    else {\n        client.EqBalQueue\n                .add('stand')\n                .add('get ' + sigil+ ' from ' +container)\n                .add('drop '+sigil)\n                .add('combine ' +sigil)\n                .add('put group ' +sigil+ ' in ' +container)\n                .send();\n    }\n};\n\nclient.sigils.dropf = function(sigil, container) {\n    sigil = client.sigils.dict[sigil] || sigil;\n    \n    client.EqBalQueue\n            .add('stand')\n            .add('get ' + sigil+ ' from ' +container)\n            .add('attach flame to ' +sigil)\n            .add('drop '+sigil)\n            .add('combine ' +sigil)\n            .add('put group ' +sigil+ ' in ' +container)\n            .send();\n};\n\nclient.sigils.attach = function(sigil, obj, container) {\n    sigil = client.sigils.dict[sigil] || sigil;\n\n    client.EqBalQueue\n            .add('stand')\n            .add('get ' + sigil+ ' from ' +container)\n            .add('attach ' +sigil+ ' to ' +obj)\n            .add('combine ' +sigil)\n            .add('put group ' +sigil+ ' in ' +container)\n            .send();\n};\n\nclient.sigils.getSigil = function(sigil, container) {\n    sigil = client.sigils.dict[sigil] || sigil;\n\n    client.EqBalQueue\n            .add('stand')\n            .add('get group ' + sigil+ ' from ' +container)\n            .add('get group ' + sigil+ ' from ' +container)\n            .add('get ' + sigil+ ' from ' +container)\n            .add('get ' + sigil)\n            .add('combine ' +sigil)\n            .add('put group ' +sigil+ ' in ' +container)\n            .send();\n};\n\n"},{"type":"alias","name":"","enabled":true,"id":3,"matching":"exact","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"action":"script","script":"client.sigils.drop('monolith', 'pack');"}],"text":"dmono"},{"type":"alias","name":"","enabled":true,"id":4,"matching":"exact","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"action":"script","script":"client.sigils.drop('eye', 'pack');"}],"text":"deye"},{"type":"alias","name":"","enabled":true,"id":5,"matching":"exact","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"action":"script","script":"client.sigils.drop('cube', 'pack');"}],"text":"dcube"},{"type":"alias","name":"","enabled":true,"id":6,"matching":"exact","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"action":"script","script":"client.sigils.store('cube', 'pack');"}],"text":"ccube"},{"type":"alias","name":"","enabled":true,"id":7,"matching":"exact","whole_words":true,"case_sensitive":true,"prefix_suffix":true,"actions":[{"action":"script","script":"client.sigils